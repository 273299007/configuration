<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration, Version=5.0.0.0, Culture=neutral, PublicKeyToken=6d32ff45e0ccc69f" />
    </configSections>
    <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
        <container>
            <register type="someType">
                <constructor>
                    <param name="defaultDependency" />
                    <param name="namedDependency" dependencyName="someName" />
                    <param name="typedDependency" dependencyType="SomeOtherType" />
                    <param name="valueDependency" value="someValue" />
                    <param name="valueWithTypeConverter" value="someValue" typeConverter="MyConverter" />
                    <param name="optionalValue">
                        <optional name="dependencyKey" type="DependencyType" />
                    </param>
                    <param name="emptyArrayValue">
                        <array />
                    </param>
                    <param name="typedEmptyArrayValue">
                        <array type="MyElementType" />
                    </param>
                    <param name="arrayWithValues">
                        <array>
                            <dependency />
                            <value value="something" />
                            <dependency name="dependencyName" />
                        </array>
                    </param>
                </constructor>
            </register>
        </container>
    </unity>
</configuration>